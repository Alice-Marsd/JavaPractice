package com.competition;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int num=0;
		String str="";
		while(scan.hasNextInt())
		{
			num=scan.nextInt();
			Competition(1,num);
		}
	}

	private static void Competition(int i,int j) {
		if(i<j)
		{
			
			System.out.print('('+i);
			Competition(i+1,j-1);
			System.out.print(','+j);
		}
	}

}
