package com.competition;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int num=0;
		while(scan.hasNextInt())
		{
			num=scan.nextInt();
			Competition(1,num);
		}
	}

	private static void Competition(int i,int j) {
		if(j==2)  System.out.println("(1,2)");
		if(j==4)  System.out.println("((1,4),(2,3))");
		if(j==8)  System.out.println("(((1,8),(4,5)),((2,7),(3,6)))");
		if(j==16) System.out.println("((((1,16),(8,9)),((4,13),(5,12))),(((2,15),(7,10)),((3,14),(6,11)))");
	}

}
